    "N = int(input())\n",
    "qtd_in = 0\n",
    "qtd_out = 0\n",
    "for i in range(N):\n",
    "    X = int(input())\n",
    "    if(10 <= X <= 20):\n",
    "        qtd_in += 1\n",
    "    else:\n",
    "        qtd_out += 1\n",
    "print(f'{qtd_in} in')\n",
    "print(f'{qtd_out} out')"
   ]
