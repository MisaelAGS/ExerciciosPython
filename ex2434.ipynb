    "qtd_dias, saldo_inicial = map(int, input().split())\n",
    "saldo_dia = saldo_inicial\n",
    "historico = []\n",
    "for i in range(qtd_dias):\n",
    "    movimentacao = int(input())\n",
    "    saldo_dia += movimentacao\n",
    "    historico.append(saldo_dia)\n",
    "print(min(historico))"
