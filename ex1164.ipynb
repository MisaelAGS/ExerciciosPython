    "N = int(input())\n",
    "soma = 0\n",
    "for i in range(N):\n",
    "    num = int(input())\n",
    "    for i in range(1, num):\n",
    "        if(num % i == 0):\n",
    "            soma += i\n",
    "    if(num == soma):\n",
    "        print(f'{num} eh perfeito')\n",
    "    else:\n",
    "        print(f'{num} nao eh perfeito')\n",
    "    soma = 0"
   ]
  }
